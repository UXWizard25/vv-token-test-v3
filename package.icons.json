{
  "name": "@marioschmidt/design-system-icons",
  "version": "1.0.2",
  "description": "BILD Design System Icons - Multi-platform icon transformation pipeline",
  "main": "dist/icons/react/index.js",
  "module": "dist/icons/react/index.js",
  "types": "dist/icons/react/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/icons/react/index.d.ts",
      "import": "./dist/icons/react/index.js",
      "default": "./dist/icons/react/index.js"
    },
    "./react": {
      "types": "./dist/icons/react/index.d.ts",
      "import": "./dist/icons/react/index.js",
      "default": "./dist/icons/react/index.js"
    },
    "./react/*": {
      "types": "./dist/icons/react/*.d.ts",
      "import": "./dist/icons/react/*.js",
      "default": "./dist/icons/react/*.js"
    },
    "./svg/*": "./dist/icons/svg/*",
    "./android/*": "./dist/icons/android/*",
    "./flutter/*": "./dist/icons/flutter/*",
    "./ios/*": "./dist/icons/ios/*",
    "./package.json": "./package.icons.json"
  },
  "files": [
    "dist/icons/",
    "README.icons.md",
    "LICENSE"
  ],
  "scripts": {
    "build:icons": "node scripts/icons/build-icons.js",
    "build:icons:svg": "node scripts/icons/optimize-svg.js",
    "build:icons:react": "node scripts/icons/generate-react.js",
    "build:icons:android": "node scripts/icons/generate-android.js",
    "build:icons:flutter": "node scripts/icons/generate-flutter.js",
    "build:icons:ios": "node scripts/icons/generate-ios.js",
    "clean:icons": "rm -rf dist/icons",
    "prepublishOnly:icons": "npm run build:icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UXWizard25/vv-token-test-v3.git"
  },
  "bugs": {
    "url": "https://github.com/UXWizard25/vv-token-test-v3/issues"
  },
  "homepage": "https://github.com/UXWizard25/vv-token-test-v3#readme",
  "keywords": [
    "icons",
    "design-system",
    "svg",
    "react-icons",
    "android-icons",
    "flutter-icons",
    "ios-icons",
    "bild",
    "figma"
  ],
  "author": "UXWizard25",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "svgo": "^3.2.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "svg2vectordrawable": "^2.9.1",
    "fantasticon": "^3.0.0",
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {}
}
